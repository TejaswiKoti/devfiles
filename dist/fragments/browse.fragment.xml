<core:FragmentDefinition xmlns="sap.m" displayBlock="true" xmlns:u="sap.ui.unified" xmlns:core="sap.ui.core">
	<Dialog id="idBrowseFrag" class="sapUiContentPadding" title="Browse file">
    
		<content>
        <u:FileUploader id="idUpload" sameFilenameAllowed="true" fileType="XLSX,xlsx"/>
        </content>
        <beginButton>

        <Button  id="btn" text="Upload" type="Neutral" press="onUpload"></Button>

        </beginButton>

        <endButton>

        <Button text="Close" type="Ghost" press="onClose"></Button>

        </endButton>
        
        
			</Dialog>

</core:FragmentDefinition>